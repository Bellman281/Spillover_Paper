***********************************************************************************************
* MC Aleer paper, VAR1 , GARCH(1,1) , VARMA variance CCC GARCH -Shanghai
**************************************************************************************************
open data LogReturn.xls
data(format=xls,org=columns) / OilRmb SG000001
compute gstart=2,gend=2354
system(model=var1)
variables OilRmb SG000001
lags 1
end(system)


garch(p=1,q=1,model=var1,mv=CC,variance=varma,pmethod=simplex,piters=5,hmatrices=Varmah, MVHSERIES=VarmaHmatrix ) gstart gend OilRmb SG000001


open copy McAl-SG000001-Hmatrix.xls
copy(data,format=xls,org=columns)  /VarmaHmatrix



